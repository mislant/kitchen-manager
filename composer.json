{
  "name": "mislant/kitchen-manager",
  "description": "Application to manage your food stock and recipes",
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap5": "^2.0",
    "vlucas/phpdotenv": "^5.5",
    "psr/container": "^2.0",
    "cycle/orm": "^2.2",
    "cycle/annotated": "^3.2",
    "psr/cache": "^3.0",
    "symfony/cache": "^6.2",
    "robmorgan/phinx": "^0.13.4",
    "ramsey/uuid": "^4.7",
    "twbs/bootstrap-icons": "^1.10"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "^3.0",
    "codeception/module-yii2": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Kitman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kitman\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
